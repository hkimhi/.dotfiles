[user]
	email = hodkimhi@gmail.com
	name = Hod Kimhi
[alias]
	logg = log --graph --oneline
	log2 = log --graph --all --decorate --pretty=format:'%C(auto)%h %d %s%n'
[init]
	defaultBranch = main
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[core]
	editor = nvim
	excludesFile = ~/.gitignore
	autocrlf = input
